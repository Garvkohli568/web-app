powershell -NoProfile -Command "@'
<!doctype html><html><body>
  <h1>Welcome to My App</h1>
  <% if (loggedIn) { %>
    <a href="/dashboard">Dashboard</a> | <a href="/logout">Logout</a>
  <% } else { %>
    <a href="/login">Login</a>
  <% } %>
</body></html>
'@ | Set-Content -Encoding UTF8 .\Views\index.ejs"
